    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "joi": "^17.11.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "resume-screening",
    "ai",
    "mistral-ai",
    "nodejs",
    "mongodb",
    "vercel"
  ],
