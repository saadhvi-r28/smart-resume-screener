{
  "name": "smart-resume-screener",
  "version": "1.0.0",
  "description": "AI-powered resume screening application with Mistral AI integration",
  "main": "api/index.js",
  "scripts": {
    "start": "node backend/src/app.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && python3 -m http.server 8080",
